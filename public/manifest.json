{
  "name": "Extension ME List",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Extension para usar ME List dentro de Mercado Libre",
  "icons": {
    "512": "regalo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "regalo.png",
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "*://*.herokuapp.com/*"
  ],
  "permissions": [
    "cookies",
    "tabs"
  ]
}